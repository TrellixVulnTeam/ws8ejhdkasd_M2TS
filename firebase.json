{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/*.psd"
    ],
    "rewrites": [ {
      "source": "/",
      "destination": "/chatbot.html"
    },
    {
      "source": "/chatbot",
      "destination": "/chatbot.html"
    },
    {
      "source": "/inteligenciaartificial",
      "destination": "/inteligenciaartificial.html"
    },
    {
      "source": "/empresa",
      "destination": "/empresa.html"
    },
    {
      "source": "/contato",
      "destination": "/contato.html"
    } ]
  }
}
